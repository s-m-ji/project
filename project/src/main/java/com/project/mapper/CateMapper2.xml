<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- mapper의 namespace속성은 연결할 mapper의 interface 풀 경로로 작성  -->
<mapper namespace="com.project.mapper.CateMapper2">


<select id="getlist" resultType="com.project.vo.CategoryVO2">

	select * from cate

</select>

<insert id="insert">

insert into cate ( bigcate, midcate, smcate)
        	   values ( #{bigcate}, #{midcate}, #{smcate} )

</insert>


</mapper>